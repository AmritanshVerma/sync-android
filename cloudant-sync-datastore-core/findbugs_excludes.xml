<!--
A list of known findbugs issues that we cannot fix at this time.
-->
<FindBugsFilter>

    <!-- A couple of classes shadow the names of their supertypes.
    These excludes can be removed when https://github.com/cloudant/sync-android/issues/252 is
    resolved.
    -->
    <Match>
        <Bug code="Nm" pattern="NM_SAME_SIMPLE_NAME_AS_SUPERCLASS" />
        <Class name="com.cloudant.sync.sqlite.ContentValues"/>
    </Match>
    <Match>
        <Bug code="Nm" pattern="NM_SAME_SIMPLE_NAME_AS_SUPERCLASS" />
        <Class name="com.cloudant.sync.util.Document"/>
    </Match>
</FindBugsFilter>
